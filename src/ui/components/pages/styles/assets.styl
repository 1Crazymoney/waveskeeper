@import '../../../styles/colors.styl';

.assets {
  width: 100%;
  position: relative;
  padding: 14px;
  height: 100%;
  background: $basic50;

  &::after {
    display: block;
    content: '';
    width: 100%;
    height: 112px;
    position: absolute;
    left: 0;
    z-index: 0;
    top: 0;
    background: -moz-linear-gradient(left, #6b94ff 0%, #1f5af6 100%);
    background: -webkit-linear-gradient(left, #6b94ff 0%, #1f5af6 100%);
    background: linear-gradient(to right, #6b94ff 0%, #1f5af6 100%);
  }
}

.logoMargin {
  margin-top: 100px;
  margin-bottom: 44px;
}
